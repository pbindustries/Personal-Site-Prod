extends ../layout

block content
  .carousel.slide#myCarousel(data-ride="carousel")
    //- Wrapper for slides
    .carousel-inner
      .item.active
        img.center.buddy1(src="/images/buddy/buddylong.jpg", alt="Pic1", height="2000", width="2000")
        .carousel-caption
          h1 Resources

  //- FIRST CONTAINER
  .container-fluid.bg-5.text-center.bot   
    div

    div.panel-group#accordion
      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse3") Top Resources

        div.panel-collapse.collapse.in#collapse3
          div.panel-body
            ul
              li
                a(href='https://www.w3schools.com/howto/default.asp') How to - w3school

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse1") Bootstrap

        div.panel-collapse.collapse.in#collapse1
          div.panel-body
            ul
              li(class=(title === 'bootstrapSnippet') ? 'active' : undefined)
                a(href='https://bootsnipp.com/') Code snippets for Bootstrap
                
      div.panel.panel-default
