extends layout

block content
  .container-fluid.text-center.bg-1
    .jumbotron(style="background:transparent !important")
      h1 Resources
      .container#hoverPic
        img.img-responsive.margin#preHoverPic(src="/images/buddy/buddy4.jpg", alt="Image", align="middle")

  //- FIRST CONTAINER
  .container-fluid.bg-5.text-center  
    div

    div.panel-group#accordion
      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse1") Bootstrap

        div.panel-collapse.collapse.in#collapse1
          div.panel-body
            ul
              li(class=(title === 'bootstrapSnippet') ? 'active' : undefined)
                a(href='https://bootsnipp.com/') Code snippets for Bootstrap
              li
                a(href='https://www.w3schools.com/bootstrap/bootstrap_carousel.asp') Bootstrap Carousel

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse2") Jade/Pug

        div.panel-collapse.collapse.in#collapse2
          div.panel-body
            ul
              li(class=(title === 'htmlToJade') ? 'active' : undefined)
                a(href='http://html2jade.aaron-powell.com/') HTML to Jade Converter
              li(class=(title === 'pugCheatsheet') ? 'active' : undefined)
                a(href='http://naltatis.github.io/jade-syntax-docs/#attributes') Jade/Pug Cheatsheet

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse3") CSS

        div.panel-collapse.collapse.in#collapse3
          div.panel-body
            ul
              li(class=(title === 'cssFormatterOptimiser') ? 'active' : undefined)
                a(href='http://www.codebeautifier.com/') CSS Formatter and Optimiser
              li(class=(title === 'menuAndNav') ? 'active' : undefined)
                a(href='http://alvit.de/css-showcase/css-navigation-techniques-showcase.php') CSS Menus and Navigation Tabs

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse4") Other

        div.panel-collapse.collapse.in#collapse4
          div.panel-body
            ul
              li(class=(title === 'fontAwesomeCheatsheet') ? 'active' : undefined)
                a(href='http://fontawesome.io/cheatsheet/') Font Awesome Cheatsheet

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse5") Node/Express

        div.panel-collapse.collapse.in#collapse5
          div.panel-body
            ul
              li
                a(href='https://github.com/jaredhanson/passport-local') Passport Local Strategy 
              li
                a(href='https://www.npmjs.com/package/express-status-monitor') Express Status Monitor  

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse6") Stocks

        div.panel-collapse.collapse.in#collapse6
          div.panel-body
            ul
              li
                a(href='https://github.com/nodesocket/quote-stream') Pushes JSON Stock Quotes In Real-Time Over Websockets
              li
                a(href='https://github.com/markmarijnissen/stockticker') Stock Ticker Web-App With Spine.js, LiveScript, Jade, Less and Brunch 
              li
                a(href='https://codepen.io/shshaw/pen/wGBzs') Stock Ticker(using only HTML, CSS and JS)
              li
                a(href='https://github.com/markmarijnissen/stockticker') Stock Ticker Web-app with Spine.js, using LiveScript, Jade, Less and build with Brunch 
              li
                a(href='https://www.jqueryscript.net/other/jQuery-Plugin-To-Display-Stock-Quotes-On-Your-Website-stockQuotes.html') jQuery Stock Quote Plugin
              li
                a(href='https://codepen.io/ecardoza/pen/jfeuA') jQuery/Google Finance Stock Ticker 
              li
                a(href='http://widgets.freestockcharts.com/scrollingticker.aspx?type=gainers') Scrolling Ticker Widgets

      div.panel.panel-default
        div.panel-heading
          h4.panel-title
            a(data-toggle="collapse", data-parent="#accordion", href="#collapse7") Tests

        div.panel-collapse.collapse.in#collapse7
          div.panel-body
            ul
              li
                a(href='https://docs.cypress.io/guides/getting-started/installing-cypress.html#npm-install') Cypress
              
  br
  br